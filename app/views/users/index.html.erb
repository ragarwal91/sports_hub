<h2>Users</h2>
<hr>
<% @users.each do |user| %>
  <%= user.id %><br>
  <%= link_to(user.username, user_path(user)) %>
  <%= "#{user.first_name} #{user.last_name}" %><br>
  <%= user.password_digest %><br>
  <%= user.email_address %>
  <%= user.password %>
  <% if session[:current_user] == user.id %>
  <%= link_to 'Remove', user_path(user), method: :delete %>
  <% end %>
  <hr>
<% end %>

<% if session[:current_user] %>
  hi
<% else %>
  bye
<% end %>
